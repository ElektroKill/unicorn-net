<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard1.1;netstandard2.0</TargetFrameworks>
    <Version>1.0.0</Version>

    <GenerateDocumentationFile Condition=" '$(Configuration)' == 'Release' ">True</GenerateDocumentationFile>

    <Description>.NET binding/wrapper for the Unicorn engine.</Description>
    <Copyright>Copyright 2018 (c) FICTURE7</Copyright>
    <Authors>FICTURE7;Gr√©goire Geis</Authors>

    <PackageId>Unicorn.Net</PackageId>
    <PackageVersion>$(Version)</PackageVersion>
    <PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
    <PackageTags>unicorn wrapper binding</PackageTags>

    <PackageProjectUrl>https://github.com/FICTURE7/unicorn-net</PackageProjectUrl>
    <PackageLicenseUrl>$(PackageProjectUrl)/blob/master/LICENSE</PackageLicenseUrl>

    <RepositoryUrl>$(PackageProjectUrl).git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

</Project>
